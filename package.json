{
  "name": "app",
  "version": "0.0.1",
  "description": "gulp-base",
  "main": "index.js",
  "scripts": {
    "start": "gulp"
  },
  "config": {
    "src": {
      "sass": "src/sass/render/*.sass",
      "html": "src/render/",
      "js": "src/js/render/*.js",
      "fonts": "src/font/**/*.*",
      "img": {
        "compress": [
          "src/img/**/*.*",
          "!src/img/noCompress/**/*.*",
          "!src/img/svg/**/*.*"
        ],
        "noCompress": "src/img/noCompress/**/*.*",
        "svg": "src/img/svg/**/*.svg"
      },
      "template": "src/template/**/*.php"
    },
    "dist": {
      "css": "dist/css",
      "html": "dist",
      "js": "dist/js",
      "fonts": "dist/font",
      "libs": "dist/libs",
      "img": "dist/img",
      "template": "dist/template"
    },
    "libs": {
      "js": "src/js/libs.js",
      "css": "src/css/libs.css"
    },
    "bem": {
      "levels": "['common.blocks', 'project.blocks']"
    }
  },
  "author": "Zanli",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.1.1",
    "browser-sync": "^2.23.7",
    "del": "^3.0.0",
    "fancybox": "^3.0.1",
    "fontfaceobserver": "^2.0.13",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cheerio": "^0.6.3",
    "gulp-cssnano": "^2.1.3",
    "gulp-file-include": "^2.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-import-css": "^0.1.3",
    "gulp-include": "^2.3.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify": "^3.0.0",
    "jquery": "^3.3.1",
    "node-bourbon": "^4.2.8",
    "node-sass-tilde-importer": "^1.0.2",
    "require-dir": "^1.0.0",
    "run-sequence": "^2.2.1",
    "slick-carousel": "^1.8.1"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.6"
  }
}
